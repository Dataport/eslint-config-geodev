{
	"name": "@dataport/eslint-config-geodev",
	"version": "1.0.0-alpha.1",
	"description": "Generic ESLint configuration for geo development projects at Dataport",
	"type": "module",
	"main": "dist/index.js",
	"exports": {
		".": {
			"types": "./dist/configs/index.d.ts",
			"default": "./dist/configs/index.js"
		},
		"./*": {
			"types": "./dist/configs/*.d.ts",
			"default": "./dist/configs/*.js"
		}
	},
	"scripts": {
		"build": "tsc",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"test": "echo OK",
		"tsc": "tsc --noEmit",
		"prepare": "husky",
		"preversion": "npm run lint && npm run test && npm run tsc && npm run build",
		"version": "conventional-changelog -r 1 -p conventionalcommits -t v && git add CHANGELOG.md"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Dataport/eslint-config-geodev.git"
	},
	"author": "Dataport <DataportSoftwareentwicklungGeo@dataport.de>",
	"license": "EUPL-1.2",
	"bugs": {
		"url": "https://github.com/Dataport/eslint-config-geodev/issues"
	},
	"homepage": "https://github.com/Dataport/eslint-config-geodev#readme",
	"files": [
		"dist/**/*",
		"README.md",
		"LICENSE"
	],
	"dependencies": {
		"@eslint/js": "^9.25.1",
		"@eslint/json": "^0.13.1",
		"@eslint/markdown": "^7.1.0",
		"@html-eslint/eslint-plugin": "^0.47.0",
		"@typescript-eslint/parser": "^8.34.1",
		"eslint-plugin-tsdoc": "^0.4.0",
		"eslint-plugin-vue": "^10.0.0",
		"eslint-plugin-vue-pug": "^1.0.0-alpha.3",
		"globals": "^16.0.0",
		"neostandard": "^0.12.1",
		"typescript-eslint": "^8.31.0",
		"vue-eslint-parser": "^10.1.3"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^20.0.0",
		"@types/eslint": "^9.6.1",
		"@types/node": "^24.7.0",
		"commitlint": "^20.1.0",
		"conventional-changelog": "^7.1.1",
		"conventional-changelog-conventionalcommits": "^9.1.0",
		"eslint": "^9.25.1",
		"husky": "^9.1.7",
		"jiti": "^2.6.1",
		"type-fest": "^5.0.1"
	},
	"peerDependencies": {
		"eslint": ">= 9"
	}
}
